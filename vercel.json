{
  "version": 2,
  "builds": [
    {
      "src": "ProjectFolder/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ProjectFolder/wsgi.py"
    }
  ]
}



